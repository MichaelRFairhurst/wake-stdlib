extern Bool is:

	Text -- toText();

extern Num is:

	Num -- modulo(Num);
	Num -- floor();
	Num -- ceil();
	Num -- round();
	Num -- squared();
	Num -- abs();

	Num -- orMaxOf(Num);
	Num -- orMaxOf(Num[]);

	Num -- orMinOf(Num);
	Num -- orMinOf(Num[]);

	Num -- numberOfDigits();

	Text -- toText();

	Num[] -- range(Num);

	Char -- fromAsciiCode();

extern Text is:

	Text -- toUpper();
	Text -- toLower();
	Text -- switchCase();

	Text -- trim();

	Num -- getSize();

	Text -- substr(Num start);
	Text -- substr(Num start, Num end);

	Num? -- parseNum();

	Char? -- charAt(Num);
	Char[] -- toCharArray();

	Text[] -- split(Text);
	Text[] -- split(Char);

	Num? -- indexOf(Text);
	Num? -- indexOf(Char);

	Text -- join(Text[]);

extern Char is:

	Char -- toUpper();
	Char -- toLower();
	Bool -- isUpper();
	Bool -- isLower();
	Bool -- isAlpha();
	Bool -- isDigit();
	Num -- toAsciiCode();

	Text -- toText();
	Text -- join(Text[]);
