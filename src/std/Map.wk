module std;

every Map{K, V} is:

	with K[] = [];
	with V[] = [];

	put($K, $V) {
		var Num? = getPosOf($K);
		if Num exists {
			V[Num] = $V;
		} else {
			K[].push($K);
			V[].push($V);
		}
	}

	V? -- get($K) {
		var Num? = getPosOf($K);
		if Num exists {
			return V[Num];
		}
		return nothing;
	}

	Num? -- getPosOf($K) {
		foreach K[] at i {
			if K == $K then return i;
		}
		return nothing;
	}

	K[] -- getKeys() {
		return K[];
	}

	V[] -- getValues() {
		return V[];
	}
